        "@youwol/kepler": "^0.1.1",
        "@youwol/math": "^0.1.0",
        "@youwol/three-extra": "^0.1.4",
        "jquery": "^3.7.1",
        "three": "^0.152.0",
        "three-stdlib": "^2.28.7",
        "tweakpane": "^4.0.1",
